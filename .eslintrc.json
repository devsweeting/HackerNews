{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "react/prefer-stateless-function": 2,
        "react/no-unused-state": 2,
        "indent": [ "warn", 2 ],
        "linebreak-style": [ "error", "unix" ],
        "quotes": [ "warn", "single" ],
        "semi": [ "warn", "always" ],
        "no-console": "warn",
        "no-unused-vars": "warn",
        "no-unused-labels": "warn",
        "react/prop-types": "warn",
        "no-undef": "warn"

    }
}
